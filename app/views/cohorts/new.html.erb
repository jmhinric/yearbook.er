<%= form_for @cohort, url: {action: "create"},
html: {class: "class_form"} do |f| %>
  Location: <%= f.text_field :location %>
  Month: <%= f.text_field :month  %>
  Year: <%= f.text_field :year %>
  <%= f.file_field :cohort_photo, placeholder:"Upload Class Photo"%>
  Nickname: <%= f.text_field :nickname %>
  <%= f.submit "Submit" %>
<% end %>